{
  "name": "bcoin",
  "version": "2.2.0",
  "description": "Bitcoin bike-shed",
  "license": "MIT",
  "repository": "git://github.com/raptoracle/bcoin.git",
  "homepage": "https://github.com/raptoracle/bcoin",
  "bugs": {
    "url": "https://github.com/raptoracle/bcoin/issues"
  },
  "author": "Fedor Indutny <fedor@indutny.com>",
  "contributors": [
    "Christopher Jeffrey <chjjeffrey@gmail.com>"
  ],
  "keywords": [
    "bcoin",
    "bitcoin",
    "blockchain",
    "cryptocurrency",
    "wallet"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "bcfg": "git+https://github.com/raptoracle/bcfg.git",
    "bcrypto": "git+https://github.com/raptoracle/bcrypto.git",
    "bcurl": "git+https://github.com/raptoracle/bcurl.git",
    "bdb": "git+https://github.com/raptoracle/bdb.git",
    "bdns": "git+https://github.com/raptoracle/bdns.git",
    "bevent": "git+https://github.com/raptoracle/bevent.git",
    "bfile": "git+https://github.com/raptoracle/bfile.git",
    "bfilter": "git+https://github.com/raptoracle/bfilter.git",
    "bheep": "git+https://github.com/raptoracle/bheep.git",
    "binet": "git+https://github.com/raptoracle/binet.git",
    "blgr": "git+https://github.com/raptoracle/blgr.git",
    "blru": "git+https://github.com/raptoracle/blru.git",
    "blst": "git+https://github.com/raptoracle/blst.git",
    "bmongo": "git+https://github.com/raptoracle/bmongo.git",
    "bmutex": "git+https://github.com/raptoracle/bmutex.git",
    "brq": "git+https://github.com/raptoracle/brq.git",
    "bs32": "git+https://github.com/raptoracle/bs32.git",
    "bsert": "git+https://github.com/raptoracle/bsert.git",
    "bsock": "git+https://github.com/raptoracle/bsock.git",
    "bsocks": "git+https://github.com/raptoracle/bsocks.git",
    "btcp": "git+https://github.com/raptoracle/btcp.git",
    "buffer-map": "git+https://github.com/raptoracle/buffer-map.git",
    "bufio": "git+https://github.com/raptoracle/bufio.git",
    "bupnp": "git+https://github.com/raptoracle/bupnp.git",
    "bval": "git+https://github.com/raptoracle/bval.git",
    "bweb": "git+https://github.com/raptoracle/bweb.git",
    "loady": "git+https://github.com/raptoracle/loady.git",
    "n64": "git+https://github.com/raptoracle/n64.git",
    "nan": "^2.19.0"
  },
  "bundleDependencies": [
    "bcfg",
    "bcrypto",
    "bcurl",
    "bdb",
    "bdns",
    "bevent",
    "bfile",
    "bfilter",
    "bheep",
    "binet",
    "blgr",
    "blru",
    "blst",
    "bmongo",
    "bmutex",
    "brq",
    "bs32",
    "bsert",
    "bsock",
    "bsocks",
    "btcp",
    "buffer-map",
    "bufio",
    "bupnp",
    "bval",
    "bweb",
    "loady",
    "n64",
    "nan"
  ],
  "devDependencies": {
    "bmocha": "git+https://github.com/raptoracle/bmocha.git"
  },
  "main": "./lib/bcoin.js",
  "bin": {
    "bcoin": "./bin/bcoin",
    "bcoin-node": "./bin/node",
    "bcoin-spvnode": "./bin/spvnode",
    "bwallet": "./bin/bwallet",
    "bcoin-cli": "./bin/bcoin-cli",
    "bwallet-cli": "./bin/bwallet-cli"
  },
  "scripts": {
    "browserify": "browserify -s bcoin lib/bcoin-browser.js | uglifyjs -c > bcoin.js",
    "clean": "rm -f {browser/,}{bcoin.js,bcoin-worker.js,app.js,worker.js}",
    "docs": "jsdoc -c jsdoc.json",
    "lint": "eslint $(cat .eslintfiles) || exit 0",
    "lint-ci": "eslint $(cat .eslintfiles)",
    "lint-file": "eslint",
    "test": "bmocha --reporter spec test/*.js",
    "test-browser": "NODE_BACKEND=js bmocha --reporter spec test/*.js",
    "test-file": "bmocha --reporter spec",
    "test-file-browser": "NODE_BACKEND=js bmocha --reporter spec",
    "test-ci": "BCOIN_RUN_LONG_TESTS=true nyc -a -n 'lib/**/*.js' --reporter=lcov --reporter=text npm run test",
    "webpack": "webpack --mode production --config webpack.browser.js",
    "webpack-browser": "webpack --mode production --config webpack.browser.js",
    "webpack-compat": "webpack --mode production --config webpack.compat.js",
    "webpack-app": "webpack --mode production --config webpack.app.js"
  },
  "browser": {
    "./lib/hd/nfkd": "./lib/hd/nfkd-compat.js",
    "./lib/hd/wordlist": "./lib/hd/wordlist-browser.js",
    "./lib/workers/child": "./lib/workers/child-browser.js",
    "./lib/workers/parent": "./lib/workers/parent-browser.js",
    "./lib/bcoin": "./lib/bcoin-browser.js"
  }
}
